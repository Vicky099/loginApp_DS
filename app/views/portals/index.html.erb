<div class="container">
	<h2>Users Data </h2>

	<div class="user_data">
		<table class="table table-bordered">
			<tr>
				<th scope="col">#</th>
				<th scope="col">Email</th>
				<th scope="col">Role</th>
			</tr>
			<% @users.each do |user| %>
				<tr>
					<td scope="row"><%= user.id %></td>
					<td scope="row"><%= user.email %></td>
					<td scope="row"><%= user.role %></td>
				</tr>
			<% end %>
		</table>
	</div>
</div>